{
    "description": "Functions Allowing Public Invocations",
    "rationale": "The Cloud Function's triggers allowed public invocations, potentially exposing undesired functionality. It is recommended that invocations be restricted to the expected principals.",
    "dashboard_name": "Bindings",
    "display_path": "functions.projects.id.functions.id",
    "path": "functions.projects.id.functions.id.bindings.id",
    "conditions": [
        "and",
        [
            "functions.projects.id.functions.id.ingress_settings",
            "equal",
            "ALLOW_ALL"
        ],
        [
            "functions.projects.id.functions.id.url",
            "notNull",
            ""
        ],
        [
            "or",
            [
                "and",
                [
                    "functions.projects.id.functions.id.gen",
                    "equal",
                    "1"
                ],
                [
                    "functions.projects.id.functions.id.bindings.id.members",
                    "containString",
                    "allUsers"
                ],
                [
                    "functions.projects.id.functions.id.bindings.id.role",
                    "equal",
                    "roles/cloudfunctions.invoker"
                ]
            ],
            [
                "and",
                [
                    "functions.projects.id.functions.id.gen",
                    "equal",
                    "2"
                ],
                [
                    "functions.projects.id.functions.id.bindings.id.members",
                    "containString",
                    "allUsers"
                ],
                [
                    "functions.projects.id.functions.id.bindings.id.role",
                    "equal",
                    "roles/run.invoker"
                ]
            ]
        ]
    ],
    "id_suffix": "bindings"
}